<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Ronin Network Examples</title>
  <link href="http://ronin-ruby.github.io/examples/network/atom.xml" rel="self" />
  <link href="http://ronin-ruby.github.io/examples/network/" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://ronin-ruby.github.io/examples/network/</id>
  <author>
    <name>Postmodern</name>
    <email>postmodern.mod3@gmail.com</email>
  </author>
  {% for example in site.categories.network %}
  <entry>
    <title>{{ example.title }}</title>
    <link href="http://ronin-ruby.github.io{{ example.url }}" />
    <updated>{{ example.date | date_to_xmlchema }}</updated>
    <id>http://ronin-ruby.github.io{{ example.url }}</id>
    <content type="html">{{ example.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
